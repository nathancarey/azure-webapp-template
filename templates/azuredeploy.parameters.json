{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "mywebapp"
    },
    "sku": {
      "value": "B1"
    },
    "linuxFxVersion": {
      "value": "DOTNETCORE|8.0"
    },
    "enableAutoUpdate": {
      "value": true
    },
    "enableApplicationInsights": {
      "value": true
    },
    "enableKeyVault": {
      "value": true
    },
    "keyVaultSku": {
      "value": "standard"
    },
    "logRetentionDays": {
      "value": 30
    }
  }
}
